  "scripts": {
    "build": "npm run clean && npm run lint && webpack --progress",
    "build:i18n": "rimraf _translations && BABEL_ENV=i18n babel lib > /dev/null",
    "build:i18n:langs": "npm run build:i18n && babel-node scripts/i18nToXliff.js",
    "clean": "rimraf dist",
    "deploy": "npm run dist && gh-pages -d dist",
    "dist": "npm test && npm run clean && NODE_ENV=production webpack --debug --config webpack.config.production.js",
    "lint": "eslint lib",
    "start": "npm run build && node server.js",
    "start:devtools": "npm run build && DEVTOOLS=true node server.js",
    "test": "mocha",
    "test:watch": "mocha -w",
    "coverage": "istanbul cover _mocha -- -R spec",
    "coverage:check": "istanbul check-coverage _mocha -- -R spec",
    "coverage:open": "istanbul cover _mocha -- -R spec && open coverage/lcov-report/*.html",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
